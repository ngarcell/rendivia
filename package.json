{
  "name": "rendivia",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "npm run pseo:generate:high-intent && npm run pseo:validate && npm run seo:validate-tools && npm run seo:quality-gate",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src",
    "video:render": "remotion render src/remotion/Root.tsx CaptionVideo out/caption.mp4",
    "remotion:worker": "node scripts/remotion-worker.mjs",
    "seo:scrape": "node scripts/seo/scrape-submagic.mjs",
    "seo:validate": "node scripts/seo/validate-data.cjs",
    "seo:validate-tools": "node scripts/seo/validate-tools.cjs",
    "seo:quality-gate": "node scripts/seo/quality-gate.cjs",
    "marketing:render": "node scripts/marketing/render-marketing-videos.cjs",
    "pseo:render-demos": "node scripts/render-pseo-demos.cjs --skip-existing",
    "pseo:validate": "node scripts/validate-pseo.cjs",
    "pseo:generate:high-intent": "ts-node scripts/generate-high-intent-pseo.ts",
    "marketing:render-landing": "remotion render src/remotion/Root.tsx LandingHeroVideo public/landing/rendivia-landing.mp4",
    "marketing:render-landing-1080p": "remotion render src/remotion/Root.tsx LandingHeroVideo1080p public/landing/rendivia-landing-1080p.mp4",
    "marketing:render-landing-1080p-webm": "remotion render src/remotion/Root.tsx LandingHeroVideo1080p public/landing/rendivia-landing-1080p.webm --codec=vp8",
    "marketing:render-landing-4k": "remotion render src/remotion/Root.tsx LandingHeroVideo4k public/landing/rendivia-landing-4k.mp4"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "3.758.0",
    "@clerk/nextjs": "^6.9.3",
    "@remotion/bundler": "4.0.417",
    "@remotion/captions": "4.0.417",
    "@remotion/google-fonts": "4.0.417",
    "@remotion/lambda": "4.0.417",
    "@remotion/media": "4.0.417",
    "@remotion/player": "4.0.417",
    "@remotion/renderer": "4.0.417",
    "@supabase/supabase-js": "^2.49.4",
    "fast-xml-parser": "^4.5.3",
    "next": "16.1.6",
    "openai": "^4.77.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "remotion": "4.0.417",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@remotion/cli": "4.0.417",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
